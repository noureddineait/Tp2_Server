@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers

@{
    Layout = "_Layout";
    ViewBag.Title = "Empty";
}
@model Tp2_Server.Models.KNN
<div class="container">
    <form method="post">
        <div class="form-floating me-3 mb-3 mt-3 col">
            <input type="text" class="form-control" asp-for="k" placeholder="Enter password confirmation">
            <label asp-for="k"></label>
            <span asp-validation-for="k" class="text-danger"></span>
        </div>
        <div class="form-floating me-3 mb-3 mt-3 col">
            <input type="text" class="form-control" asp-for="distance" placeholder="Enter password confirmation">
            <label asp-for="distance"></label>
            <span asp-validation-for="distance" class="text-danger"></span>
        </div>
        <button type="submit" class="btn btn-primary">Information</button>
    </form>
</div>